#: registration/serializers.py:53
msgid "View is not defined, pass it as a context variable"
msgstr "Изгледът не е дефиниран, предайте го като контекстна променлива"

#: registration/serializers.py:58
msgid "Define adapter_class in view"
msgstr "Дефинирайте adapter_class в изгледа"

#: registration/serializers.py:77
msgid "Define callback_url in view"
msgstr "Дефинирайте callback_url в изгледа"

#: registration/serializers.py:81
msgid "Define client_class in view"
msgstr "Дефинирайте client_class в изгледа"

#: registration/serializers.py:102
msgid "Incorrect input. access_token or code is required."
msgstr "Грешен вход. Изисква се access_token или code."

#: registration/serializers.py:111
msgid "Incorrect value"
msgstr "Грешна стойност"

#: registration/serializers.py:140
msgid "A user is already registered with this e-mail address."
msgstr "Вече съществува потребител с този имейл адрес."

#: registration/serializers.py:148
msgid "The two password fields didn't match."
msgstr "Двете полета за паролата не съвпадат."

#: registration/views.py:91
msgid "ok"
msgstr "ОК"

#: serializers.py:30
msgid "Must include \"email\" and \"password\"."
msgstr "Трябва да включите \"имейл\" и \"парола\"."

#: serializers.py:41
msgid "Must include \"username\" and \"password\"."
msgstr "Трябва да включите \"потребителско име\" и \"парола\"."

#: serializers.py:54
msgid "Must include either \"username\" or \"email\" and \"password\"."
msgstr "Трябва да включите или \"потребителско име\", или \"имейл\" и \"парола\"."

#: serializers.py:95
msgid "User account is disabled."
msgstr "Потребителският акаунт е деактивиран."

#: serializers.py:98
msgid "Unable to log in with provided credentials."
msgstr "Невъзможност за вход с предоставените данни."

#: serializers.py:107
msgid "E-mail is not verified."
msgstr "Имейлът не е потвърден."

#: views.py:126
msgid "Successfully logged out."
msgstr "Успешно излязохте."

#: views.py:174
msgid "Password reset e-mail has been sent."
msgstr "Изпратен е имейл за нулиране на паролата."

#: views.py:200
msgid "Password has been reset with the new password."
msgstr "Паролата е нулирана с новата парола."

#: views.py:222
msgid "New password has been saved."
msgstr "Новата парола е запазена."

#~ msgid "Error"
#~ msgstr "Грешка"